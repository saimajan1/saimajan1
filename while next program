#include <iostream>
#include <iomanip> 
using namespace std;
int main()
{
int a=1; 
int numb=1; 
while( a<10000 ) //loop while power <= 4 digits
{
cout << setw(2) << numb; //display number
cout << setw(5) << a << endl; //display fourth power
++numb; 
a = numb*numb*numb*numb;
}
cout << endl;
return 0;
}
