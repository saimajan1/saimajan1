//generates table
# include <iostream>
# include <iomanip>                                               
using namespace std;
int main()                      //function name main()
using namespace std;
{
    cout<<1990<<setw(9)<<135<<endl;
    cout<<1991<<setw(9)<<7290<<endl;
    cout<<1992<<setw(9)<<11300<<endl;
    cout<<1993<<setw(9)<<16200<<endl;
    return 0;
}
